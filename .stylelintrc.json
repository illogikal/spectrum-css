{
  "extends": ["stylelint-config-standard"],
  "plugins": [
   "./custom-stylelint-plugins/css-logical-properties.js",
   "./custom-stylelint-plugins/no-missing-parenthesis.js",
   "./custom-stylelint-plugins/spectrum-class-name-rule.js",
   "./custom-stylelint-plugins/no-missing-var.js"
  ],
  "rules": {
    "rule-empty-line-before": null,
    "block-no-empty": null,
    "at-rule-empty-line-before": null,
    "at-rule-no-unknown": null,
    "selector-class-pattern": null,
    "no-descending-specificity": null,
    "declaration-empty-line-before": null,
    "custom-property-empty-line-before": null,
    "comment-whitespace-inside": null,
    "comment-empty-line-before": null,
    "no-duplicate-selectors": null,
    "property-no-vendor-prefix": null,
    "max-nesting-depth": 2,
    "custom-rule/no-logical-properties": true,
    "custom-rule/no-missing-parenthesis": true,
    "custom-rule/spectrum-class-name": true,
    "custom-rule/no-missing-var":true
  }
}
